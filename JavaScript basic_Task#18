//Write a JavaScript program to check two given numbers and return true if one of the number is 50 or if their sum is 50

[JS]
let first = 24;
let second = 26;

function trySum(fNum, sNum){
 if((fNum == 50 || sNum == 50) || (fNum + sNum == 50)){
  console.log("1"); 
 } else {
   console.log("2"); 
 }
}
trySum(first, second);
